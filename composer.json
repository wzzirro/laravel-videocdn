{
  "name": "awesomecinema/laravel-videocdn",
  "description": "A Laravel wrapper for VideoCDN",
  "require": {
    "php": "^7.2",
    "illuminate/support": "~6.2",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wzzirro\\Videocdn\\VideoCdnServiceProvider"
      ],
      "aliases": {
        "videocdn": "Wzzirro\\Videocdn\\Facades\\VideoCdn"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Wzzirro\\Videocdn\\": "src/"
    }
  },
  "minimum-stability": "dev"
}